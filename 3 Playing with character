#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char c;
    char s[100],sen[100];
    
    scanf("%c\n",&c);
    scanf("%[^\n]%*c",s);
    scanf("%[^\n]%*c",sen);
    printf("%c\n%s\n%s\n", c, s , sen);
    return 0;
}
